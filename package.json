  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "jasmine": "^2.4.1",
    "jsdoc": "^3.4.0",
    "jsdoc-to-markdown": "^1.3.4"
  }
